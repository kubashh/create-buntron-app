{
  "name": "myapp",
  "version": "0.0.1",
  "author": "nick",
  "description": "example",
  "scripts": {
    "dev": "buntron",
    "build": "buntron build",
    "postinstall": "electron-builder install-app-deps"
  },
  "buntron": {
    "name": "myapp",
    "appId": "com.nick.myapp",
    "targets": {
      "linux": [
        "x64"
      ],
      "mac": [],
      "win": [
        "x64",
        "ia32"
      ],
      "web": true
    },
    "core": {
      "devTools": "right",
      "quitCtrlQ": "dev",
      "quitCtrlW": "dev",
      "width": 900,
      "height": 700
    }
  },
  "devDependencies": {
    "@types/node": "22.19.0",
    "@types/react": "latest",
    "typescript": "latest",
    "buntron": "latest",
    "wdwh": "latest"
  }
}
